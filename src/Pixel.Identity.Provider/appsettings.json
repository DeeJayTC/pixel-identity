{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "MongoDbSettings": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "pixel-identity-db"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=pixel-identity-db;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "UsePlugin": "Pixel.Identity.Store.Mongo",
  "IdentityHost": "http://localhost:44382",
  "ALLOWED_ORIGINS": "http://localhost:44382",
  "AllowedHosts": "*"
}
